        edge u_close goto Close;
        edge u_stop goto Stop;
      location Close:
        initial not InitialOpen;
        edge u_open goto Open;
        edge u_stop goto Stop;
      location Stop:
        edge u_close goto Close;
        edge u_open goto Open;
    end
